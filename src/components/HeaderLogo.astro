---
const {imageType, width, height} = Astro.props;
---	
<img id = "logo" src = {imageType} />


<style define:vars ={{width, height}}>
    #logo{
		width:var(--width);
		height:var(--height);
		position:absolute;
		top:35px;
		left:35%;
	}

    /* Small Devices, Tablets */
    @media only screen and (min-width : 768px) {
        #logo{
            width:var(--width);
            height:var(--height);
            position:absolute;
            top:35px;
            left:50%;
        }
    }
    
</style>